.menu5 
{
padding:0px 0 0 0px; 
margin:0; 
list-style:none; 
height:0px; 
font-size:11px;
height:29px; 
position:relative; 


}

.menu5 li 
{
float:right; 
margin-right:2px; 
}




.menu5 li a {
display:block;
float:left; 
line-height:28px;
color:#333; 
text-decoration:none; 
font-family:arial, verdana, sans-serif; 
font-weight:bold; 
text-align:center; 
padding:0 0 0 4px; 
cursor:pointer; 
background:url(../imagens/pro_five_0a.gif) no-repeat;
}
.menu5 li a b 
{
float:left; 
display:block; 
padding:0 25px 1px 25px; 
background:url(../imagens/pro_five_0b.gif) no-repeat right top;
}
.menu5 li.current a 
{

background:url(../imagens/pro_five_2a.gif) no-repeat;
color:#FFFFFF;
font-weight:bold; 
}

.menu5 li.current a b 
{
background:url(../imagens/pro_five_2b.gif) no-repeat right top;
}



.menu5 li a:hover
{
background: url(../imagens/pro_five_2a.gif) no-repeat left top;
}


.menu5 li a:hover b
{
background: url(../imagens/pro_five_2b.gif) no-repeat right top;
color:#FFFFFF;
font-weight:bold; 

}
